package Ejercicio4;

import java.util.Random;

public class Liebre implements Competidor{
	
	public String nombre() {
		return "Liebre";
	}
	
	public Suceso suceso (int Numero) {
		
		if(Numero>=0&&Numero<=1) {
			return Suceso.DUERME;
		}else if(Numero>=2&&Numero<=3) {
			return Suceso.GRAN_SALTO;
		}else if(Numero==3) {
			return Suceso.RESBALON_GRANDE;
		}else if(Numero>=4&&Numero<=7) {
			return Suceso.PEQUENO_SALTO;
		}else {
			return Suceso.RESBALON_PEQUENO;
		}
		
		
		
	}
}		
